{
  "name": "@poc-mitosis/library",
  "private": true,
  "scripts": {
    "dev": "watch \"npm run build\" ./src",
    "prebuild": "npm run build:design-tokens",
    "build": "mitosis build --c mitosis.config.cjs",
    "lint": "eslint",
    "build:design-tokens": "style-dictionary build --config design-tokens.config.json"
  },
  "type": "module",
  "exports": {
    "./*": "./output/*/src/index.js"
  },
  "dependencies": {
    "@builder.io/eslint-plugin-mitosis": "0.0.16",
    "@builder.io/mitosis": "0.4.3",
    "@builder.io/mitosis-cli": "0.4.3"
  },
  "devDependencies": {
    "eslint": "9.10.0",
    "style-dictionary": "4.1.1",
    "watch": "^0.13.0"
  }
}
